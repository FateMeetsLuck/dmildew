function makeCounter(name) {
	let i = 0;
	let count = function() {
		i = i + 1;
		console.log(`${name} ${i}`);
	};
	return count;
}
  
var counter = makeCounter("first");
counter(); // first 1
counter(); // first 2
var counter2 = makeCounter("second");
counter2(); // second 1
let i = 6;
counter(); // first 3
counter2(); // second 2

