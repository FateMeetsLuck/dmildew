try {
    foo = 22;
} catch(e) {
    console.error(`Caught exception: ${e}`);
}

try {
    let foo = [1, 2];
    console.log(foo[2]);
} catch(e) {
    console.error(e);
}

try {
    throw "it back";
    console.log("Didn't throw");
} finally {
    console.log("Always do this");
}

