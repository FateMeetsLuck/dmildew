function getRandomNumber(min, max) {
    return min + Math.floor(Math.random() * (max-min-1));
}

console.log("Getting 10 random numbers between 1 and 10");
for(let i = 0; i < 10; i += 1) {
    console.log(getRandomNumber(1, 10));
}
