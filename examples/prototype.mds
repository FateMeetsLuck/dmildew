function Person(name, age) {
    this.name = name;
    this.age = age;
}

Person.prototype.building = "Brick house";
Person.prototype.greet = function() {
    console.log(this.name + " says hello!");
};

var p1 = new Person("Santa", 79);

console.log(p1);

var foo = {};
Person.call(foo, "Moron", 666);
console.log(foo);

