// The :? operator is given the correct precedence so as not to need parentheses for equality tests.
var foo = 9;
console.log(foo * foo == 81 ? "Correct" : "Something's wrong");
console.log(foo - foo + 1? "Correct" : "Something wrong");
console.log(5 + 5 == 4 + 6 ? "Correct" : "Something is wrong with ==");
